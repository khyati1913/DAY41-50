#include <stdio.h>
#include <ctype.h>

int main() {
    char s[1000];
    if (fgets(s, sizeof(s), stdin) == NULL) return 0;

    int vowels = 0, consonants = 0;
    for (int i = 0; s[i] != '\0' && s[i] != '\n'; i++) {
        char ch = s[i];
        if (isalpha(ch)) {
            ch = tolower(ch);
            if (ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u') {
                vowels++;
            } else {
                consonants++;
            }
        }
    }
    printf("Vowels=%d, Consonants=%d\n", vowels, consonants);
    return 0;
}
