#include <stdio.h>

int main() {
    char str[1000];
    int count = 0;

    // Read string (including whitespace, but for simplicity assume no newline)
    fgets(str, sizeof(str), stdin);

    // Count until the end of string is reached (‘\0’)
    while (str[count] != '\0' && str[count] != '\n') {
        count++;
    }

    printf("%d\n", count);
    return 0;
}
