Q91: Remove all vowels from a string.

/*
Sample Test Cases:
Input 1:
education
Output 1:
dctn

*/

#include <stdio.h>

int isVowel(char ch) {
    // Check lowercase and uppercase vowels
    if (ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' ||
        ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U')
        return 1;
    return 0;
}

int main() {
    char s[1000];
    if (fgets(s, sizeof(s), stdin)==NULL) return 0;

    // Remove newline if any
    int len = 0;
    while (s[len] != '\0' && s[len] != '\n') len++;
    s[len] = '\0';

    char res[1000];
    int j = 0;
    for (int i = 0; i < len; i++) {
        if (!isVowel(s[i])) {
            res[j++] = s[i];
        }
    }
    res[j] = '\0';

    printf("%s\n", res);
    return 0;
}
