#include <stdio.h>

int main() {
    char s[1000];
    if (fgets(s, sizeof(s), stdin) == NULL) return 0;
    int i = 0;
    while (s[i] != '\0' && s[i] != '\n') {
        if (s[i] >= 'a' && s[i] <= 'z') {
            s[i] = s[i] - ('a' - 'A');  // or âˆ’32
        }
        i++;
    }
    printf("%s", s);
    return 0;
}
